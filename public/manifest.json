{
    "manifest_version": 3,
    "name": "GitHub Helper",
    "version": "1.0.0",
    "description": "A Chrome Extension built with React and TypeScript",
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "github.com"
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ]
}